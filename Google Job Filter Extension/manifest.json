{
  "manifest_version": 3,
  "name": "LinkedIn Promoted Hider",
  "version": "0.0.2",
  "description": "Hides promoted content on LinkedIn.",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["content.js"],
      "css":["content.css"]
    },
    {
      "matches": ["https://www.linkedin.com/jobs/*"],
      "js": ["content.js"]
    }
  ]
}
