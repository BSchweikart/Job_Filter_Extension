{
    "manifest_version": 3,
    "name": "LinkedIn Promoted Hider",
    "version": "0.0.1",
    "description": "Hides promoted content on LinkedIn.",
    "permissions": [
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/*"],
        "js": ["content.js"]
      }
    ]
  }
